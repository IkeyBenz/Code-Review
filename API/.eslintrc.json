{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "moment": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "func-names": "off",
        "no-underscore-dangle": "off",
        "no-console": "off"
    }
}